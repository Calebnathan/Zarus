<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements"
        xsi="http://www.w3.org/2001/XMLSchema-instance"
        engine="UnityEngine.UIElements"
        editor="UnityEditor.UIElements"
        noNamespaceSchemaLocation="../../../UIElementsSchema/UIElements.xsd"
        editor-extension-mode="False">

    <ui:Style src="../../Styles/MainTheme.uss" />

    <ui:VisualElement name="EndMenuRoot" class="overlay-backdrop">
        <ui:VisualElement name="MenuPanel" class="menu-panel slide-up">
            <ui:Label name="OutcomeTitleLabel" class="menu-title" text="MISSION COMPLETE" />
            <ui:Label name="OutcomeSubtitleLabel" class="menu-subtitle" text="Review your path and jump back in." />

            <ui:VisualElement name="OutcomeStats" class="end-stats">
                <ui:Label name="StatsDaysLabel" class="end-stat" text="Days elapsed: --" />
                <ui:Label name="StatsCureLabel" class="end-stat" text="Cure progress: --" />
                <ui:Label name="StatsProvincesLabel" class="end-stat" text="Provinces saved: --" />
                <ui:Label name="StatsOutpostsLabel" class="end-stat" text="Outposts: --" />
                <ui:Label name="StatsZarLabel" class="end-stat" text="Budget remaining: --" />
            </ui:VisualElement>

            <ui:VisualElement name="ButtonGroup" class="menu-button-group">
                <ui:Button name="RestartButton" class="button button--primary menu-button" text="Restart" />
                <ui:Button name="MenuButton" class="button button--secondary menu-button" text="Main Menu" />
                <ui:Button name="ExitButton" class="button button--danger menu-button" text="Exit" />
            </ui:VisualElement>
        </ui:VisualElement>
    </ui:VisualElement>

</ui:UXML>
